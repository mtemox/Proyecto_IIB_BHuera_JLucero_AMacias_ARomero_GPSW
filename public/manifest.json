{
    "manifest_version": 3,
    "name": "Key+",
    "version": "1.0",
    "description": "Un gestor de contraseñas seguro y fácil de usar.",
    "key": "",
    "permissions": [
        "storage",
        "identity"
    ],
    "oauth2": {
        "client_id": "768609595455-04ig0fere5g7dp1e6kpc0ljn8jiai5na.apps.googleusercontent.com",
        "scopes": [
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/userinfo.profile"
        ]
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
        "16": "key-icon-16.png",
        "48": "key-icon-48.png",
        "128": "key-icon-128.png"
        }
    },
    "icons": {
        "16": "key-icon-16.png",
        "48": "key-icon-48.png",
        "128": "key-icon-128.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}